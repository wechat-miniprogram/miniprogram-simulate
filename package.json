{
  "name": "miniprogram-simulate",
  "version": "1.6.1",
  "description": "tools for miniprogram custom component unit test",
  "main": "dist/miniprogram_simulate.cjs.js",
  "module": "dist/miniprogram_simulate.esm.js",
  "types": "dist/miniprogram_simulate.d.ts",
  "browser": "dist/miniprogram_simulate.all.ts",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "npm run test:jest && npm run test:karma",
    "test:jest": "jest",
    "test:karma": "node ./dist/serve.js & karma start",
    "coverage": "jest --coverage",
    "codecov": "jest --coverage && codecov",
    "lint": "eslint \"src/**/*.js\" --fix && eslint \"test/**/*.js\" --fix"
  },
  "keywords": [
    "miniprogram",
    "custom component",
    "unit test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wechat-miniprogram/miniprogram-simulate.git"
  },
  "author": "wechat-miniprogram",
  "license": "MIT",
  "devDependencies": {
    "@magic-works/commonjs-import.meta": "^1.0.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@rollup/plugin-wasm": "^6.1.3",
    "@types/koa": "^2.13.9",
    "@types/koa-send": "^4.1.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^20.6.3",
    "@types/path-browserify": "^1.0.0",
    "@webassemblyjs/ast": "^1.11.6",
    "@webassemblyjs/wasm-parser": "^1.11.6",
    "codecov": "^3.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "glass-easel-miniprogram-adapter": "^0.2.1",
    "glass-easel-template-compiler": "^0.2.1",
    "koa": "^2.14.2",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "mime": "^3.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^7.0.23"
  }
}
